<?xml version="1.0" encoding="utf-8" standalone="no"?><manifest xmlns:android="http://schemas.android.com/apk/res/android" android:installLocation="auto" package="com.olsoft.sa.ntvplus">
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.CALL_PHONE"/>
    <uses-permission android:name="android.permission.SEND_SMS"/>
    <uses-permission android:name="android.permission.READ_CONTACTS"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    <application android:icon="@drawable/ntv_launcher_icon" android:label="@string/app_name" android:name="com.olsoft.sa.ntvplus.NTVPlusApplication" android:theme="@style/ntvplus">
        <activity android:name="com.olsoft.sa.alt.RootActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="com.olsoft.sa.alt.SettingsActivity"/>
        <activity android:name="com.olsoft.sa.alt.AboutActivity"/>
        <activity android:name="com.olsoft.sa.alt.MenuActivity"/>
        <activity android:name="com.olsoft.sa.alt.SendCommandActivity"/>
        <activity android:name="com.olsoft.sa.alt.NewsActivity"/>
        <activity android:name="com.olsoft.sa.alt.FavoritesActivity"/>
        <activity android:launchMode="singleTop" android:name="com.olsoft.sa.alt.SearchActivity">
            <intent-filter>
                <action android:name="android.intent.action.SEARCH"/>
            </intent-filter>
            <meta-data android:name="android.app.searchable" android:resource="@xml/searchable"/>
        </activity>
        <meta-data android:name="android.app.default_searchable" android:value="com.olsoft.sa.alt.SearchActivity"/>
        <provider android:authorities="com.olsoft.sa.ntvplus.SearchSuggestionsProvider" android:name="com.olsoft.sa.ntvplus.SearchSuggestionsProvider"/>
        <receiver android:exported="true" android:name="com.google.android.apps.analytics.AnalyticsReceiver">
            <intent-filter>
                <action android:name="com.android.vending.INSTALL_REFERRER"/>
            </intent-filter>
        </receiver>
    </application>
</manifest>